TITLE
!  TEST LATICE
!     BEAM, PARTICLE=MUON, MASS=0.10565838934, CHARGE=1.0,ENERGY=15, &
!    EX=2.0E-05,EY=2.0E-05,KBUNCH=6, NPART=1.0E+08, &
!    KBUNCH=6, NPART=1.0E+08

BRHO:= 50.0

 C:= 100.0
 RHO:= C/(2*PI)
 NDIP := 5.0
 LDIP := C/5.0
 ANG :=  twopi/NDIP
 B0   := (ANG * BRHO)/ LDIP


 
!COMBINED FUNCTION DIPOLES:
 n:= 0.5
 
 GDD1:= -(B0 * n)/RHO


 BD : SBEND, L= LDIP, ANGLE = ANG, K1= GDD1/BRHO                


 CELL: LINE= (BD)
 RSRT:  LINE = (5*(CELL))




